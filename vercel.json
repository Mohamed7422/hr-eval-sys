{
  "version": 2,
  "builds": [
    {
      "src": "hr_evaluation/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "maxLambdaSize": "15mb"
      }
    }
  ],
    "routes": [
    { "src": "/static/(.*)", "dest": "staticfiles/$1" },
    { "src": "/(.*)","dest": "hr_evaluation/wsgi.py" }
  ],
  "buildCommand": "python manage.py collectstatic --noinput && python manage.py migrate"


}
