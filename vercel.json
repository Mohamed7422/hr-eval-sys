{
  "version": 2,
  "builds": [
    {
      "src": "hr_evaluation/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "buildCommand": "python manage.py collectstatic --noinput",
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)","dest": "hr_evaluation/wsgi.py" }
  ]

}
